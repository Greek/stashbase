{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "dev": "nodemon --exec \"node -r esbuild-register ./src/index.ts\" -e .ts",
    "lint": "tsc --noEmit && eslint \"src/**/*.ts*\" --max-warnings 0",
    "start": "node -r esbuild-register ./dist/index.js",
    "test": "jest --detectOpenHandles"
  },
  "dependencies": {
    "@t3-oss/env-core": "^0.13.4",
    "@trpc/server": "^11.1.2",
    "better-auth": "^1.2.8",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "esbuild-register": "^3.5.0",
    "express": "^5.1.0",
    "express-http-context": "1.2.4",
    "express-ruid": "^1.1.5",
    "ioredis": "^5.6.1",
    "morgan": "^1.10.0",
    "pg": "^8.16.0",
    "winston": "^3.17.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@stashbase/eslint-config": "workspace:*",
    "@stashbase/jest-presets": "workspace:*",
    "@stashbase/typescript-config": "workspace:*",
    "@types/body-parser": "^1.19.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.17.47",
    "@types/pg": "^8.15.1",
    "@types/supertest": "^6.0.2",
    "drizzle-kit": "^0.31.1",
    "esbuild": "^0.20.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "prettier": "^3.5.3",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "5.7.2"
  },
  "packageManager": "pnpm@10.10.0"
}